{
  "name": "laravel/laravel",
  "description": "Admin for LVA Referee Administrators.",
  "minimum-stability": "stable",
  "keywords": [
    "framework",
    "laravel",
    "volleyball",
    "referee",
    "admin",
    "london"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Giulio Troccoli-Allard",
      "email": "giulio@troccoli.it"
    }
  ],
  "type": "project",
  "require": {
    "php": ">=7.1",
    "barryvdh/laravel-debugbar": "^2.4",
    "codescheme/postcodes": "^1.3",
    "davejamesmiller/laravel-breadcrumbs": "3.0.2",
    "doctrine/dbal": "^2.5",
    "laracasts/flash": "^3.0",
    "laravel/framework": "5.4.*",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^5.4"
  },
  "require-dev": {
    "appzcoder/crud-generator": "^2.0",
    "fzaninotto/faker": "~1.4",
    "laracademy/interactive-make": "^1.1",
    "laravel/dusk": "^1.0",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7",
    "symfony/css-selector": "^3.0",
    "symfony/dom-crawler": "^3.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "LVA\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
